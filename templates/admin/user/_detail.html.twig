<div class="form-group-static">
    <label class="col-sm-2">Username</label>
    <div class="col-sm-10">
        {{ user.username }}
    </div>
</div>
<div class="form-group-static">
    <label class="col-sm-2">Name</label>
    <div class="col-sm-10">
        {{ user.displayname }}
    </div>
</div>
<div class="form-group-static">
    <label class="col-sm-2">Email</label>
    <div class="col-sm-10">
        {{ user.email }}
    </div>
</div>
<div class="form-group-static">
    <label class="col-sm-2">Roles</label>
    <div class="col-sm-10">
        {{ (user.roles is empty) ? 'Access Denied' : user.roleLabels|join(', ') }}
    </div>
</div>
