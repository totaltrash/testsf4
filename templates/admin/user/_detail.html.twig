<table class="table">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ user.id }}</td>
        </tr>
        <tr>
            <th>Username</th>
            <td>{{ user.username }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ user.displayname }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ user.email }}</td>
        </tr>
        <tr>
            <th>Enabled</th>
            <td>{{ user.enabled ? 'Yes' : 'No' }}</td>
        </tr>
    </tbody>
</table>
