    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    {{ encore_entry_link_tags('app') }}
    {% block stylesheets %}{% endblock %}
    <title>{% block title %}Application{% endblock %}</title>
  </head>
    <body>
        <div id="app">
            {% block body %}{% endblock %}
